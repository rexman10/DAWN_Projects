  <body>

    <div class="container-fluid"  id="circuitos">
      <div class="container-fluid">

        <div id="carouselCircuitos" class="carousel carousel-dark slide" data-ride="carousel">
          <ol class="carousel-indicators">

          </ol>
          <div class="carousel-inner">

          </div>
          <a class="carousel-control-prev" href="#carouselCircuitos" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
          </a>
          <a class="carousel-control-next" href="#carouselCircuitos" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
          </a>
        </div>
      </div>
    </div>


    <div class="container-fluid" id="busquedaPilotos">
      <div class="form-group">
        <h1>Busqueda de pilotos por año:</h1>
        <label for="anio">Ingrese un año</label>
        <input type="text" class="form-control" name="anio" id="anio" aria-describedby="helpId" placeholder="Ejemplo: 2003">
      </div>
      <button type="submit" class="btn btn-primary" name="buscarPiloto">Buscar</button>
    </div>

    <div class="container-fluid" id="tablaPilotos">
      <table class="table table-dark" >
        <thead>
          <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Numero</th>
          </tr>
        </thead>
        <tbody name="displayPilotos">
  
        </tbody>
      </table>
    </div>

<div class="container-fluid" id="graficos">
  <div class="container-fluid" id="botonesFiltro">
    <button type="button" name="anterior" id="anterior" class="btn btn-primary" btn-lg btn-block">Ronda Anterior</button>
    <button type="button" name="siguiente" id="siguiente" class="btn btn-primary" btn-lg btn-block">Siguiente Ronda</button>
  </div>
  <ul class="charts-css legend legend-circle legend-inline" name="legend">

  </ul>
  <table class="charts-css line multiple show-heading hide-data show-labels show-data-axes show-primary-axis show-8-secondary-axes legend legend-circle" id="linePitstops">
    <caption id="circuitoHolder"> Tiempos de vuelta </caption>
    <tbody name="cuerpoTabla">

    </tbody>
  
  </table>
</div>

